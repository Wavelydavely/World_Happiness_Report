CREATE TABLE world_happiness_report_2021 (
	Country_name VARCHAR,
	Regional_indicator VARCHAR,
	Ladder_score DEC,
	Standard_error_of_ladder_score DEC,
	upperwhisker DEC,
	lowerwhisker DEC,
	Logged_GDP_per_capita DEC,
	Social_support DEC,
	Healthy_life_expectancy DEC,
	Freedom_to_make_life_choices DEC,
	Generosity DEC,
	Perceptions_of_corruption DEC,
	Ladder_score_in_Dystopia DEC,
	Explained_by_Log_GDP_per_capita DEC,
	Explained_by_Social_support DEC,
	Explained_by_Healthy_life_expectancy DEC,
	Explained_by_Freedom_to_make_life_choices DEC,
	Explained_by_Generosity DEC,
	Explained_by_Perceptions_of_corruption DEC,
	Dystopia_residual DEC
);

SELECT * FROM world_happiness_report_2021 LIMIT 10;
