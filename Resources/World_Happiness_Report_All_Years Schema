DROP TABLE IF EXISTS World_Happiness_Report_All_Years;

CREATE TABLE World_Happiness_Report_All_Years (
	Country VARCHAR,
	Year INT,
	Happiness_Score DEC,
	Economic_Production DEC,
	Social_Support DEC,
	Life_Expectancy DEC,
	Freedom DEC,
	Generosity DEC,
	Perceptions_of_Corruption DEC
);

SELECT * FROM World_Happiness_Report_All_Years LIMIT 10;